{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "prefer-destructuring": [
      "error",
      {
        "array": false,
        "object": true
      }
    ]
  }
}
